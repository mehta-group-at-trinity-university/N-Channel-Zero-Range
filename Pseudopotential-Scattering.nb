Cell[CellGroupData[{

Cell["The K-matrix for the multichannel problem.", "Subsection",
 CellChangeTimes->{{3.75802437518563*^9, 3.758024391376979*^9}}],

Cell["\<\
Let\[CloseCurlyQuote]s now return to the LS equation and match to the \
following asymptotic form:\
\>", "Text",
 CellChangeTimes->{{3.758025280803376*^9, 3.758025324537342*^9}, {
  3.7580262620023327`*^9, 3.758026263198614*^9}}],

Cell[BoxData[
 FormBox[
  RowBox[{
   SubscriptBox["M", 
    RowBox[{"i", ",", "j"}]], "\[Rule]", 
   RowBox[{
    RowBox[{
     RowBox[{
      SubscriptBox["f", "i"], 
      SubscriptBox["\[Delta]", "ij"]}], "-", 
     RowBox[{
      SubscriptBox["g", "i"], 
      SubscriptBox["K", "ij"]}]}], "\[Rule]", " ", 
    RowBox[{
     RowBox[{"sin", "(", 
      RowBox[{
       SubscriptBox["k", "i"], "r"}], ")"}], "+", 
     RowBox[{
      SubscriptBox["K", 
       RowBox[{"i", " ", "j"}]], 
      RowBox[{"cos", "(", 
       RowBox[{
        SubscriptBox["k", "i"], "r"}], ")"}]}]}]}]}], 
  TraditionalForm]], "EquationNumbered",
 CellChangeTimes->{{3.758025308454749*^9, 3.758025320104472*^9}, {
  3.7580262650790358`*^9, 3.75802629597719*^9}, {3.758026331899119*^9, 
  3.758026343618227*^9}, {3.7580266693109818`*^9, 3.758026702780581*^9}}],

Cell[TextData[{
 "Where ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["f", "i"], ",", 
    SubscriptBox["g", "i"]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " satisfy the large ",
 Cell[BoxData[
  FormBox["r", TraditionalForm]],
  FormatType->"TraditionalForm"],
 " channel equations."
}], "Text",
 CellChangeTimes->{{3.758026346169846*^9, 3.75802636818351*^9}}],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    SubscriptBox["u", "i"], "(", 
    StyleBox["r",
     FontWeight->"Bold"], ")"}], "=", 
   RowBox[{
    RowBox[{
     SubscriptBox["\[Delta]", "ij"], "r", " ", 
     SuperscriptBox["\[ExponentialE]", 
      RowBox[{"\[ImaginaryI]", " ", 
       SubscriptBox["k", "j"], "z"}]]}], "-", 
    RowBox[{
     SuperscriptBox["\[ExponentialE]", 
      RowBox[{"\[ImaginaryI]", " ", 
       SubscriptBox["k", "i"], "r"}]], 
     RowBox[{
      UnderscriptBox["\[Sum]", 
       RowBox[{"j", "'"}]], 
      RowBox[{
       SubscriptBox["A", 
        RowBox[{"i", " ", 
         RowBox[{"j", "'"}]}]], 
       RowBox[{
        SubscriptBox["u", 
         RowBox[{"j", "'"}]], "'"}], 
       RowBox[{"(", "0", ")"}]}]}]}]}]}], 
  TraditionalForm]], "EquationNumbered",
 CellChangeTimes->{3.758026426051322*^9}],

Cell[TextData[{
 "Using the Rayleigh formula for the plane wave: ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["\[ExponentialE]", 
     RowBox[{"\[ImaginaryI]", " ", "k", " ", "z"}]], "=", 
    RowBox[{
     OverscriptBox[
      UnderscriptBox["\[Sum]", 
       RowBox[{"l", "=", "0"}]], "\[Infinity]"], 
     RowBox[{
      RowBox[{
       SuperscriptBox["\[ImaginaryI]", "l"], "(", 
       RowBox[{
        RowBox[{"2", "l"}], "+", "1"}], ")"}], 
      RowBox[{
       SubscriptBox["j", "l"], "(", 
       RowBox[{"k", " ", "r"}], ")"}], 
      RowBox[{
       SubscriptBox["P", "l"], "(", 
       RowBox[{"cos", " ", "\[Theta]"}], ")"}]}]}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 ", and the Euler formula for ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["\[ExponentialE]", 
    RowBox[{"\[ImaginaryI]", " ", 
     SubscriptBox["k", "i"], " ", "r"}]], TraditionalForm]],
  FormatType->"TraditionalForm"]
}], "Text",
 CellChangeTimes->{{3.7580264868244257`*^9, 3.758026495102903*^9}, {
  3.758026548466366*^9, 3.758026568193141*^9}}],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    SubscriptBox["u", "i"], "(", 
    StyleBox["r",
     FontWeight->"Bold"], ")"}], "=", 
   RowBox[{
    RowBox[{
     SubscriptBox["\[Delta]", "ij"], 
     RowBox[{
      OverscriptBox[
       UnderscriptBox["\[Sum]", 
        RowBox[{"l", "=", "0"}]], "\[Infinity]"], 
      RowBox[{
       RowBox[{
        SuperscriptBox["\[ImaginaryI]", "l"], "(", 
        RowBox[{
         RowBox[{"2", "l"}], "+", "1"}], ")"}], "r", " ", 
       RowBox[{
        SubscriptBox["j", "l"], "(", 
        RowBox[{
         SubscriptBox["k", "j"], " ", "r"}], ")"}], 
       RowBox[{
        SubscriptBox["P", "l"], "(", 
        RowBox[{"cos", " ", "\[Theta]"}], ")"}]}]}]}], "-", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       RowBox[{"cos", "(", 
        RowBox[{
         SubscriptBox["k", "i"], "r"}], ")"}], "+", 
       RowBox[{"\[ImaginaryI]", " ", 
        RowBox[{"sin", "(", 
         RowBox[{
          SubscriptBox["k", "i"], "r"}], ")"}]}]}], ")"}], 
     RowBox[{
      UnderscriptBox["\[Sum]", 
