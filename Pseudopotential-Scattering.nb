       SubscriptBox["V", "kj"]}]}], "=", 
     RowBox[{
      RowBox[{
       UnderscriptBox["\[Sum]", "k"], 
       RowBox[{
        SubscriptBox["G", "i"], 
        SubscriptBox["\[Delta]", "ik"], 
        SubscriptBox["V", "kj"]}]}], "=", 
      RowBox[{
       SubscriptBox["G", "i"], 
       SubscriptBox["V", "ij"]}]}]}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
