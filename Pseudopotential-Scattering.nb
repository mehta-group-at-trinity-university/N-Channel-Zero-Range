Cell[TextData[{
 "Now what does ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["f", 
    RowBox[{"1", "\[Rule]", "2"}]], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " signify for the case of only channel 1 open and channel 1 the entrance \
channel?  Let\[CloseCurlyQuote]s go back to the LS equation:"
}], "Text",
 CellChangeTimes->{{3.758022566317943*^9, 3.7580226105836487`*^9}}],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    SubscriptBox["u", "i"], "(", 
    StyleBox["r",
     FontWeight->"Bold"], ")"}], "=", 
   RowBox[{
    RowBox[{
     RowBox[{
      SubscriptBox["\[Delta]", "ij"], "r", " ", 
      SuperscriptBox["\[ExponentialE]", 
       RowBox[{"\[ImaginaryI]", " ", 
        SubscriptBox["k", "j"], "z"}]]}], "-", 
     RowBox[{
      SuperscriptBox["\[ExponentialE]", 
       RowBox[{"\[ImaginaryI]", " ", 
        SubscriptBox["k", "i"], "r"}]], 
      RowBox[{
       UnderscriptBox["\[Sum]", 
        RowBox[{"j", "'"}]], 
       RowBox[{
        SubscriptBox["A", 
         RowBox[{"i", " ", 
          RowBox[{"j", "'"}]}]], 
        RowBox[{
         SubscriptBox["u", 
          RowBox[{"j", "'"}]], "'"}], 
        RowBox[{"(", "0", ")"}]}]}]}]}], "=", 
    RowBox[{
     RowBox[{"-", 
      SuperscriptBox["\[ExponentialE]", 
       RowBox[{
        RowBox[{"-", 
         SubscriptBox["\[Kappa]", "i"]}], "r"}]]}], 
     RowBox[{
      UnderscriptBox["\[Sum]", 
       RowBox[{"j", "'"}]], 
      RowBox[{
       SubscriptBox["A", 
        RowBox[{"i", " ", 
         RowBox[{"j", "'"}]}]], 
       RowBox[{
        SubscriptBox["u", 
         RowBox[{"j", "'"}]], "'"}], 
       RowBox[{"(", "0", ")"}]}]}]}]}]}], 
  TraditionalForm]], "EquationNumbered",
 CellChangeTimes->{{3.7580226116745462`*^9, 3.758022638554775*^9}}],

Cell["So the coefficient to the closed-channel wavefunction is:", "Text",
