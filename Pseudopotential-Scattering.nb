Cell[BoxData[
 FormBox[
  RowBox[{
   SubscriptBox["D", "i"], "=", 
   RowBox[{
    SubscriptBox["f", 
     RowBox[{"j", "\[Rule]", "i"}]], "=", 
    RowBox[{"-", 
     RowBox[{
      UnderscriptBox["\[Sum]", 
       RowBox[{"j", "'"}]], 
      RowBox[{
       SubscriptBox["A", 
        RowBox[{"i", " ", 
         RowBox[{"j", "'"}]}]], 
       RowBox[{
        SubscriptBox["u", 
         RowBox[{"j", "'"}]], "'"}], 
       RowBox[{"(", "0", ")"}]}]}]}]}]}], 
